{
  "compilerOptions": {
  "target": "es5",
  "module": "CommonJS",
  "strict": true,
  "esModuleInterop": true,
  "noImplicitAny": true,
  "removeComments": true,
  "preserveConstEnums": true,
  "outDir": "build",
  "sourceMap": true
  },
  "include": [
  "src/**/*"
  ],
  "exclude": [
  "node_modules",
  "**/*.spec.ts"
  ]
  }